<Window x:Class="AdvanceHomework.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AdvanceHomework"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="login" HorizontalAlignment="Left" Margin="281,140,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="179" Height="28"/>
        <TextBox x:Name="password" HorizontalAlignment="Left" Margin="281,189,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="179" Height="28"/>
        <Button x:Name="LogIn" Content="Login" HorizontalAlignment="Left" Margin="281,264,0,0" VerticalAlignment="Top" Height="31" Width="179" Click="LogInClick"/>

    </Grid>
</Window>
